{
  "name": "solid-router",
  "version": "0.1.5",
  "description": "A declarative router for solid-js",
  "keywords": [
    "solid-js",
    "router",
    "solid-router",
    "client side routing"
  ],
  "author": "Le Minh Duc<duclm@sendo.vn>",
  "license": "MIT",
  "main": "dist/dom/index.cjs.js",
  "module": "dist/dom/index.esm.js",
  "typings": "types/client.d.js",
  "sideEffect": false,
  "scripts": {
    "build": "yarn build:types && yarn build:js",
    "build:js": "rollup -c",
    "build:types": "tsc",
    "watch": "rollup -c -w",
    "prebuild": "rm -rf dist"
  },
  "files": [
    "dist",
    "server",
    "hydrate",
    "types"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mduclehcm/solid-router.git"
  },
  "bugs": {
    "url": "https://github.com/mduclehcm/solid-router/issues"
  },
  "homepage": "https://github.com/mduclehcm/solid-router#readme",
  "peerDependencies": {
    "solid-js": "^0.15.5"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-typescript": "^7.7.7",
    "@types/history": "^4.7.3",
    "babel-preset-solid": "^0.15.4",
    "rollup": "^1.27.14",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-filesize": "^6.2.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "solid-js": "^0.15.5",
    "typescript": "^3.7.4"
  },
  "dependencies": {
    "history": "^4.10.1",
    "path-to-regexp": "^6.1.0"
  }
}
